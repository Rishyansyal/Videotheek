extend ../layout        

block content 
    form(method= "post" action="/users/"+ user.customer_id +"/edit")
  div(class="mb-3")
    label(for="exampleInputEmail1" class="form-label") Email address
    input(type="email" class="form-control" value= user.email id="exampleInputEmail1" aria-describedby="emailHelp")
  button(type="submit" class="btn btn-primary") Submit